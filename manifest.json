{
  "manifest_version": 3,
  "name": "Quibble",
  "version": "1.0.0",
  "description": "A Chrome extension to overlay Figma files on top of any website",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": ["activeTab", "storage"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["content.css"],
      "js": ["content.js"]
    }
  ],
  "devtools_page": "devtools.html"
}
